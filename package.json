{
  "name": "disckeeperthree",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "critical": "sass --style compressed --no-source-map src/sass/critical.scss src/css/critical/critical.css",
    "sass-compile": "sass --style compressed --no-source-map src/sass/dk.scss src/css/dk.css",
    "sass-version": "nodeRev 'src/css/dk.css' --hash -o 'src/css/rev' ",
    "sass-remove": "src/.bin/sass-file-remove.sh",
    "sass": "npm-run-all sass-compile sass-version sass-remove",
    "home-page-sprite": "node src/.bin/home-page.js",
    "backmin": "terser src/js-inline/backbutton.js -o src/js-inline/mins/backbutton-min.js",
    "buttonmin": "terser src/js-inline/menubutton.js -o src/js-inline/mins/menubutton-min.js",
    "foragemin": "terser src/js-inline/localForage-config.js -o src/js-inline/mins/localForage-config-min.js",
    "createplayermin": "terser src/js-inline/createplayer.js -o src/js-inline/mins/createplayer-min.js",
    "createcoursemin": "terser src/js-inline/createcourse.js -o src/js-inline/mins/createcourse-min.js",
    "adjustparsmin": "terser src/js-inline/adjustpars.js -o src/js-inline/mins/adjustpars-min.js",
    "roundsetupmin": "terser src/js-inline/roundsetup.js -o src/js-inline/mins/roundsetup-min.js",
    "roundscoringmin": "terser src/js-inline/roundscoring.js -o src/js-inline/mins/roundscoring-min.js",
    "roundhistorymin": "terser src/js-inline/roundhistory.js -o src/js-inline/mins/roundhistory-min.js",
    "statisticsmin": "terser src/js-inline/statistics.js -o src/js-inline/mins/statistics-min.js",
    "navcheckmin": "terser src/js-inline/navcheck.js -o src/js-inline/mins/navcheck-min.js",
    "listplayersmin": "terser src/js-inline/listplayers.js -o src/js-inline/mins/listplayers-min.js",
    "listcoursesmin": "terser src/js-inline/listcourses.js -o src/js-inline/mins/listcourses-min.js",
    "servicemin": "terser disckeeper-service-worker.js -o disckeeper-service-worker-min.js",
    "serve": "rm -rf dist && npx @11ty/eleventy --serve",
    "build": "rm -rf dist && npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "node-rev": "^1.3.0",
    "node-sprite-generator": "^0.10.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.51.0",
    "terser": "^5.15.1"
  }
}
